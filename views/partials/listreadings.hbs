<table class="ui table">
  <thead>
    <tr>
      <th>Code</th>
      <th>Temp</th>
      <th>Wind Speed</th>
      <th>Pressure</th>
      <th>Wind Direction</th>
    </tr>
  </thead>
  <tbody>
     {{#each station.readings}}
    <tr>
      <td>
        {{code}}
      </td>
      <td>
        {{temp}}
      </td>
      <td>
        {{windspeed}}
      </td>
      <td>
       {{pressure}}
      </td>
      <td>
       {{winddir}}
      </td>
    </tr>
     {{/each}}
  </tbody>
</table>